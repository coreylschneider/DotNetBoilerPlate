{
	"General_Settings": {
		"debug_enabled": true
	},
	"SemanticMemory": {
		"ConnectionString": "YOUR_COSMOS_DB_CONNECTION_STRING",
		"DatabaseName": "SKMemory",
		"CollectionName": "generic"
	},
	"Api_Configs": {
		"Azure": {
			"AZURE_API_KEY": "YOUR_AZURE_API_KEY"
		},
		"OpenAI": {
			"OPENAI_API_KEY": "YOUR_OPENAI_API_KEY",
			"Org_ID": "YOUR_OPENAI_ORG_ID",
			"model": "gpt-3.5-turbo",
			"max_tokens": 1024,
			"base_url": "https://api.openai.com/v1/",
			"documentation": {
				"doc_base": "https://platform.openai.com/docs"
			}
		},
		"Google": {
			"GOOGLE_API_KEY": "YOUR_GOOGLE_API_KEY",
			"base_url": "https://www.googleapis.com/customsearch/v1",
			"documentation": {
				"doc_base": "https://developers.google.com/custom-search/v1/overview"
			}
		},
		"Groq": {
			"GROQ_API_KEY": "YOUR_GROQ_API_KEY",
			"base_url": "https://api.groq.com/openai/v1/",
			"documentation": {
				"doc_base": "https://console.groq.com/docs/quickstart"
			}
		},
		"Serper": {
			"SERPER_API_KEY": "YOUR_SERPER_API_KEY",
			"base_url": "https://google.serper.dev/",
			"documentation": {
				"doc_base": "https://serper.io/docs/"
			}
		},
		"Github": {
			"GITHUB_API_KEY": "YOUR_GITHUB_API_KEY",
			"GITHUB_API_KEY_CLASSIC": "YOUR_GITHUB_CLASSIC_API_KEY",
			"base_url": "https://api.github.com/",
			"documentation": {
				"doc_base": "https://docs.github.com/en/rest",
				"auth": "/authentication/authenticating-to-the-rest-api"
			}
		},
		"Kraken": {
			"KRAKEN_API_KEY": "YOUR_KRAKEN_API_KEY",
			"KRAKEN_PRIVATE_KEY": "YOUR_KRAKEN_PRIVATE_KEY"
		},
		"Anthropic": {
			"Anthropic_API_KEY": "YOUR_ANTHROPIC_API_KEY",
			"base_url": "https://api.anthropic.com/v1/",
			"documentation": {
				"doc_base": "https://docs.anthropic.com/en/api/getting-started"
			}
		},
		"DeepSeek": {
			"DeepSeek_API_KEY": "YOUR_DEEPSEEK_API_KEY",
			"base_url": "https://api.deepseek.com",
			"documentation": {
				"doc_base": "https://api-docs.deepseek.com/",
				"pricing": "https://api-docs.deepseek.com/quick_start/pricing"
			}
		}
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.Hosting.Lifetime": "Warning"
		}
	}
}
